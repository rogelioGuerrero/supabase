{
  "name": "backend-api-supabase",
  "version": "1.0.0",
  "description": "Backend API for Productos using Supabase",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config dist/app.js",
    "dev": "nodemon -r dotenv/config src/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "netlify:build": "npm install -g typescript; npm install; npm run build; npm prune --production",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "supabase",
    "api",
    "productos"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@supabase/supabase-js": "^2.48.1",
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "serverless-http": "^3.2.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  }
}
